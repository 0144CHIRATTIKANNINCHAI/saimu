<h1 class="pb-4">Saved Place</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(savedplace.length > 0) { %>
    <% savedplace.forEach(function(temple) { %>
      <a href="/temple/<%= temple._id %>" class="col text-center category__link">
        <div class="category__img category__img--large shadow">
          <img src="/img/หน้าเปล่า.png" alt="<%= temple.สถานที่ %>" loading="lazy">
        </div>
        <div class="pt-1"><%= temple.สถานที่ %></div>
      </a>
    <% }) %>
  <% } else { %>
    <p>No items found.</p>
  <% } %> 
</div>
